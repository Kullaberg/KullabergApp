{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "off",
        "no-unreachable": "warn",
        "no-unused-vars": "off",
        "constructor-super": "warn",
        "valid-typeof": "warn",
        "comma-dangle": "off",
        "prefer-arrow-callback": "off"
    },
    "globals": {
        "Materialize": true,
        "intervalID": true,
        "jQuery": true,
        "$": true,
        "window": true,
        "navigator": true
    }

}
